/* pages/pendingExchangeList/pendingExchangeList.wxss */
.container {
  padding: 10px 20px;
}

.item {
  width: 100%;
  display: flex;
  justify-content: space-between;
  align-items: center;
  backdrop-filter: blur(5px);
  padding: 20px 5px;
  border-radius: 10px;
  box-shadow: 0px 0px 5px 5px rgba(0, 0, 0, 0.05);
  margin: 10px 0px;
}
.info {
  display: flex;
  flex-direction: column;
}
.name{
  padding: 10px 0;
  font-weight: bolder;
}
.time{
  font-size: 14px;
}
